{
  "name": "CSN downloader",
  "description": "specific for beta",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "back/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end",
      "js": [
        "back/contentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "csn/*.js"
  ],
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 2
}
